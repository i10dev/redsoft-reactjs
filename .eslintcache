ex-1\">\n        <h1 className=\"content__label\">Картины эпохи Возрождения</h1>\n        <div className=\"content__collection flex flex-between\">\n          {cardList &&\n            cardList.map(el => (\n              <Card\n                key={el.id}\n                {...el}\n                actionClick={() => buyItem(el.id)}\n                setBtnText\n              />\n            ))}\n        </div>\n      </div>\n      <footer className=\"footer wrapper\">\n        <div className=\"footer__inner wrapper flex flex-between flex-center\">\n          <Logo src={logoDark} />\n          <Menu list={navItem} />\n          <div className=\"contacts flex flex-1 flex-end\">\n            <a className=\"footer__tel\" href=\"tel:+74955555555\">\n              +7 (495) 555-55-55\n            </a>\n            <address className=\"footer__address\">\n              г. Москва, ул. Расплетина, 24\n            </address>\n          </div>\n        </div>\n      </footer>\n    </div>\n  );\n}\n\nexport default App;\n","/home/user/Документы/project/redsoft-reactjs/src/index.js",[],["42","43"],"/home/user/Документы/project/redsoft-reactjs/src/Components/Search/Search.jsx",[],"/home/user/Документы/project/redsoft-reactjs/src/Components/Menu/Menu.jsx",[],"/home/user/Документы/project/redsoft-reactjs/src/Components/Logo/Logo.jsx",[],"/home/user/Документы/project/redsoft-reactjs/src/Components/index.js",[],"/home/user/Документы/project/redsoft-reactjs/src/Components/Card/Card.jsx",[],"/home/user/Документы/project/redsoft-reactjs/src/Components/Button/Button.jsx",[],"/home/user/Документы/project/redsoft-reactjs/src/db.js",[],{"ruleId":"44","severity":1,"message":"45","line":37,"column":10,"nodeType":"46","messageId":"47","endLine":37,"endColumn":16},{"ruleId":"44","severity":1,"message":"48","line":37,"column":18,"nodeType":"46","messageId":"47","endLine":37,"endColumn":27},{"ruleId":"49","replacedBy":"50"},{"ruleId":"51","replacedBy":"52"},"no-unused-vars","'busket' is assigned a v